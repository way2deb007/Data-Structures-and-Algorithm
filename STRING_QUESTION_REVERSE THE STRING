int main()
{
    char arr[]={'h','e','l','l','o'};
    int start=0;
    int end=4;
    while(start<end){
        char temp=arr[start];
        arr[start]=arr[end];
        arr[end]=temp;
        start++;
        end--;
    }
    for(int i=0;i<5;i++){
        cout<<arr[i]<<" ";
    }
    return 0;
}
